{
    "name": "wyrihaximus/react-child-process-messenger",
    "description": "Messenger decorator for react/child-process",
    "license": "MIT",
    "authors": [
        {
            "name": "Cees-Jan Kiewiet",
            "email": "ceesjank@gmail.com",
            "homepage": "http://wyrihaximus.net/"
        }
    ],
    "require": {
        "php": "^7.4",
        "ext-json": "^7.4",
        "cakephp/utility": "^3.4",
        "doctrine/inflector": "^1.0",
        "evenement/evenement": "^3.0 || ^2.0 || ^1.0",
        "indigophp/hash-compat": "^1.0",
        "paragonie/random_compat": "^9.0 || ^2.0",
        "react/child-process": "^0.6",
        "react/promise": "^2.8",
        "react/promise-stream": "^1.2",
        "react/promise-timer": "^1.5",
        "react/socket": "^1",
        "thecodingmachine/safe": "^1.3",
        "wyrihaximus/file-descriptors": "^1.1",
        "wyrihaximus/json-throwable": "^4 || ^3 || ^2 || ^1.1.1",
        "wyrihaximus/ticking-promise": "^2 || ^1.4"
    },
    "require-dev": {
        "wyrihaximus/async-test-utilities": "^3.3.1"
    },
    "config": {
        "platform": {
            "php": "7.4.7"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "WyriHaximus\\React\\ChildProcess\\Messenger\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "WyriHaximus\\React\\Tests\\ChildProcess\\Messenger\\": "tests/"
        },
        "files": [
            "examples/ExamplesChildProcess.php"
        ]
    },
    "scripts": {
        "post-install-cmd": [
            "composer normalize"
        ],
        "post-update-cmd": [
            "composer normalize"
        ]
    }
}
